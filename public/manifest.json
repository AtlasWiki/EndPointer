{
    "manifest_version": 3,
    "name": "Placeholder - Cross Browser Extention",
    "version": "0.1",
    "description": "A cross-browser extension for Chrome and Firefox for DOM logging and js parsing",
    "permissions": ["activeTab"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ]
  }