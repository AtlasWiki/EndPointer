{
  "manifest_version": 3,
  "name": "JS-Toolkit",
  "version": "1.0.0",
  "description": "A JS-Parsing Toolkit with many flexible features",
  "permissions": [
    "storage",
    "activeTab",
    "tabs"
  ],
  "action": {
    "default_popup": "popup/popup.html",
    "default_title": "JS Toolkit for Hackers"
  },
  "background": {
    "service_worker": "popup/background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["popup/content.js"]
    }
  ],
  "devtools_page": "devtool/devtool.html"
}