{
    "manifest_version": 2,
    "name": "Placeholder - Cross Browser Extention",
    "version": "1.0",
    "description": "A cross-browser extension for Chrome and Firefox for DOM logging and js parsing",
    "permissions": ["activeTab"],
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
    "browser_action": {
      "default_popup": "popup.html"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ]
}